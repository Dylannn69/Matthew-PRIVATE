local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Dylannn69/dylan-ui/refs/heads/main/Library.lua"))()
local window = library:AddWindow("Dylan Private", {main_color = Color3.fromRGB(150, 01, 1), min_size = Vector2.new(600, 420), toggle_key = Enum.KeyCode.LeftControl})

-- Services
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local rSto = game:GetService("ReplicatedStorage") 

local features = window:AddTab("Farm++")

local switch = features:AddSwitch("Anti Lag (Black)", function(State)
    local lighting = game:GetService("Lighting")
    if State then
        for _, gui in pairs(LocalPlayer.PlayerGui:GetChildren()) do
            if gui:IsA("ScreenGui") then gui:Destroy() end
        end
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("ParticleEmitter") or obj:IsA("PointLight") or obj:IsA("SpotLight") or obj:IsA("SurfaceLight") then
                obj:Destroy()
            end
        end
        for _, v in pairs(lighting:GetChildren()) do
            if v:IsA("Sky") then v:Destroy() end
        end
        local darkSky = Instance.new("Sky")
        darkSky.Name = "DarkSky"
        darkSky.SkyboxBk = "rbxassetid://0"
        darkSky.SkyboxDn = "rbxassetid://0"
        darkSky.SkyboxFt = "rbxassetid://0"
        darkSky.SkyboxLf = "rbxassetid://0"
        darkSky.SkyboxRt = "rbxassetid://0"
        darkSky.SkyboxUp = "rbxassetid://0"
        darkSky.Parent = lighting
        lighting.Brightness = 0
        lighting.ClockTime = 0
        lighting.TimeOfDay = "00:00:00"
        lighting.OutdoorAmbient = Color3.new(0,0,0)
        lighting.Ambient = Color3.new(0,0,0)
        lighting.FogColor = Color3.new(0,0,0)
        lighting.FogEnd = 100
        task.spawn(function()
            while State do
                task.wait(5)
                if not lighting:FindFirstChild("DarkSky") then
                    darkSky:Clone().Parent = lighting
                end
            end
        end)
    end
end)

local switch = features:AddSwitch("Anti Lag (Light)", function(State)
    if State then
        -- Disable particles and effects
        for _, v in pairs(game:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
                v.Enabled = false
            end
        end

        -- Optimize lighting
        local lighting = game:GetService("Lighting")
        lighting.GlobalShadows = false
        lighting.FogEnd = 9e9
        lighting.Brightness = 0

        -- Lower graphics quality
        settings().Rendering.QualityLevel = 1

        -- Optimize textures and materials
        for _, v in pairs(game:GetDescendants()) do
            if v:IsA("Decal") or v:IsA("Texture") then
                v.Transparency = 1
            elseif v:IsA("BasePart") and not v:IsA("MeshPart") then
                v.Material = Enum.Material.SmoothPlastic
                if v.Parent and (v.Parent:FindFirstChild("Humanoid") or v.Parent.Parent:FindFirstChild("Humanoid")) then
                    -- Keep player parts as they are
                else
                    v.Reflectance = 0
                end
            end
        end

        -- Disable post-processing effects
        for _, v in pairs(lighting:GetChildren()) do
            if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
                v.Enabled = false
            end
        end
        
        print("âœ… Light Anti-Lag Activated!")
    else
        -- Optional: Re-enable effects when toggled off
        for _, v in pairs(game:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
                v.Enabled = true
            end
        end
        
        local lighting = game:GetService("Lighting")
        lighting.GlobalShadows = true
        lighting.Brightness = 1
        
        for _, v in pairs(lighting:GetChildren()) do
            if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
                v.Enabled = true
            end
        end
        
        print("ðŸ›‘ Light Anti-Lag Deactivated!")
    end
end)

local switch = features:AddSwitch("Position Lock", function(bool)
if bool then
local currentPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
getgenv().posLock = game:GetService("RunService").Heartbeat:Connect(function()
if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = currentPos
end
end)
else
if getgenv().posLock then
getgenv().posLock:Disconnect()
getgenv().posLock = nil
end
end
end)

switch:Set(false)

local autoSpinWheelEnabled = false

local function autoSpinWheel()
    while autoSpinWheelEnabled do
        game:GetService("ReplicatedStorage").rEvents.openFortuneWheelRemote:InvokeServer("openFortuneWheel", game:GetService("ReplicatedStorage").fortuneWheelChances["Fortune Wheel"])
        task.wait(1) -- Adjust delay if needed
    end
end

features:AddSwitch("Auto Spin Fortune Wheel", function(state)
    autoSpinWheelEnabled = state
    if state then task.spawn(autoSpinWheel) end
end)


local features = window:AddTab("Rebirths")

-- ===== TARGET REBIRTH SYSTEM =====
local targetRebirth = 0
local autoRebirthActive = false

local targetInput = features:AddTextBox("Type Target Rebirth here", function(input)
    local target = tonumber(input)
    if target and target > 0 then
        targetRebirth = target
        print("[System] Target Rebirth set to:", targetRebirth)
    else
        warn("[System] Invalid number! Use format: 1500")
    end
end, {
    placeholder = "Example: 1500",
    clear_on_focus = true
})

local function autoRebirth()
    local player = game:GetService("Players").LocalPlayer
    local rebirths = player.leaderstats.Rebirths
    local rebirthRemote = game:GetService("ReplicatedStorage").rEvents.rebirthRemote
    
    while autoRebirthActive and rebirths.Value < targetRebirth do
        rebirthRemote:InvokeServer("rebirthRequest")
        task.wait(0.05) -- Optimal speed

        if rebirths.Value >= targetRebirth then
            autoRebirthActive = false
            print("[System] Success! Reached target:", targetRebirth)
            break
        end
    end
end

features:AddSwitch("Auto Rebirth To Target", function(state)
    if state then
        if targetRebirth > 0 then
            autoRebirthActive = true
            task.spawn(autoRebirth)
        else
            warn("[System] Set a valid target number first!")
        end
    else
        autoRebirthActive = false
    end
end)

local autoRebirthEnabled = false

local function autoRebirth()
    while autoRebirthEnabled do
        game:GetService("ReplicatedStorage").rEvents.rebirthRemote:InvokeServer("rebirthRequest")
        task.wait(0.1) -- Adjust delay if needed
    end
end

features:AddSwitch("Auto Rebirth (INFINITE)", function(state)
    autoRebirthEnabled = state
    if state then task.spawn(autoRebirth) end
end)

local sizeChangeEnabled = false

local function changeSize()
    while sizeChangeEnabled do
        game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
        task.wait(0.1) -- Adjust delay if needed
    end
end
features:AddSwitch("Auto Size 2", function(state)
    sizeChangeEnabled = state
    if state then task.spawn(changeSize) end
end)

local teleportEnabled = false  

local function teleportToMuscleKing()  
    while teleportEnabled do  
        local player = game.Players.LocalPlayer  
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then  
            player.Character.HumanoidRootPart.CFrame = CFrame.new(-8661.864, 17.278, -5730.793)  
        end  
        task.wait(1) -- Adjust delay if needed  
    end  
end
features:AddSwitch("Auto TP  Muscle King Gym", function(state)  
    teleportEnabled = state  
    if state then task.spawn(teleportToMuscleKing) end  
end)

-- Hide Frames
features:AddSwitch("Hide Frames", function(state)
for _, obj in pairs(rSto:GetChildren()) do
if obj.Name:match("Frame$") then
obj.Visible = not state
end
end
end)

features:AddLabel("-----| ROCKS V2 (BETTER)|-----").TextSize = 20

-- ===== ROCK FARM v2 =====

local selectrock = ""

-- 8. Muscle King Gym Rock (Full Original Code)
features:AddSwitch("Auto MuscleKingGymRock[5m]", function(Value)
    selectrock = "Muscle King Gym Rock"
    getgenv().autoFarm = Value
    while getgenv().autoFarm do
        task.wait()
        if game:GetService("Players").LocalPlayer.Durability.Value >= 5000000 then
            for i, v in pairs(game:GetService("Workspace").machinesFolder:GetDescendants()) do
                if v.Name == "neededDurability" and v.Value == 5000000 and game.Players.LocalPlayer.Character:FindFirstChild("LeftHand") and game.Players.LocalPlayer.Character:FindFirstChild("RightHand") then
                    firetouchinterest(v.Parent.Rock, game:GetService("Players").LocalPlayer.Character.RightHand, 0)
                    firetouchinterest(v.Parent.Rock, game:GetService("Players").LocalPlayer.Character.RightHand, 1)
                    firetouchinterest(v.Parent.Rock, game:GetService("Players").LocalPlayer.Character.LeftHand, 0)
                    firetouchinterest(v.Parent.Rock, game:GetService("Players").LocalPlayer.Character.LeftHand, 1)
                    for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                        if v.Name == "Punch" and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                        end
                    end
                    game:GetService("Players").LocalPlayer.muscleEvent:FireServer("punch", "leftHand")
                    game:GetService("Players").LocalPlayer.muscleEvent:FireServer("punch", "rightHand")
                end
            end
        end
    end
end)

features:AddLabel(" ")

local function autoFastPunch()
    local player = game:GetService("Players").LocalPlayer

    while _G.fastPunch do
        -- Equip Punch tool
        local punchTool = player.Backpack:FindFirstChild("Punch")
        if punchTool then
            punchTool.Parent = player.Character
            task.wait(0.1)
        end

        -- Speed up the punch animation
        local character = player.Character
        local punch = character and character:FindFirstChild("Punch")
        if punch and punch:FindFirstChild("attackTime") then
            punch.attackTime.Value = 0 -- Removes punch delay
        end

        -- Fire the punch event
        player.muscleEvent:FireServer("punch", "leftHand")
        player.muscleEvent:FireServer("punch", "rightHand")
        task.wait(0.001) -- Faster speed
    end

    -- Reset animation speed when turned off
    local punch = player.Character and player.Character:FindFirstChild("Punch")
    if punch and punch:FindFirstChild("attackTime") then
        punch.attackTime.Value = 0.35 -- Reset to normal speed
    end

    -- Unequip Punch when toggled off
    if player.Character:FindFirstChild("Punch") then
        player.Character.Punch.Parent = player.Backpack
    end
end

-- Fast Punch Toggle
local switch = features:AddSwitch("Fast Punch", function(bool)
    _G.fastPunch = bool
    if bool then
        autoFastPunch()
    end
end)
switch:Set(false) -- Default off

features:AddButton("Anti-AFK", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/hassanxzayn-lua/Anti-afk/main/antiafkbyhassanxzyn"))();
end)
